{
  "name": "promod",
  "version": "0.0.44",
  "description": "Library for automation testing",
  "main": "./built/index.js",
  "scripts": {
    "test": "mocha $(find specs/swd -name '*.spec.*') --require ts-node/register --timeout 500000",
    "test:units": "mocha $(find units -name '*.test.*') --require ts-node/register --timeout 500000",
    "test:debug": "mocha ./specs/async.child.methods.spec.ts --require ts-node/register --timeout 300000",
    "lint": "eslint --ext .ts ./",
    "tsc": "rm -rf ./built && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simple-Automation-Testing/promod.git"
  },
  "author": {
    "name": "Potapov Dmytro",
    "url": "https://github.com/potapovDim"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Simple-Automation-Testing/promod/issues"
  },
  "homepage": "https://github.com/Simple-Automation-Testing/promod#readme",
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "assertior": "0.0.26",
    "eslint": "^7.29.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-mocha": "^8.2.0",
    "mocha": "10.0.0",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "playwright": "^1.27.1"
  },
  "dependencies": {
    "sat-utils": "0.3.2",
    "selenium-standalone": "^7.1.0",
    "selenium-webdriver": "4.5.0",
    "yargs": "^17.5.1"
  },
  "keywords": [
    "promod",
    "selenium-webdriver",
    "protractor",
    "protractor mod",
    "test automation",
    "selenium",
    "automation testing"
  ]
}