{
  "name": "promod",
  "version": "1.1.0-rc-18",
  "description": "Library for automation testing",
  "main": "./built/index.js",
  "bin": "./bin/promod_cli",
  "scripts": {
    "pw:install": "PLAYWRIGHT_BROWSERS_PATH=0 playwright install --force chrome",
    "test": "mocha $(find specs -name '*.spec.*') --require ts-node/register --timeout 500000",
    "test:pw": "mocha $(find specs/pw -name '*.spec.*') --require ts-node/register --timeout 500000",
    "test:swd": "mocha $(find specs/swd -name '*.spec.*') --require ts-node/register --timeout 500000",
    "test:units": "mocha $(find units -name '*.test.*') --require ts-node/register --timeout 500000",
    "test:debug": "mocha ./specs/async.child.methods.spec.ts --require ts-node/register --timeout 300000",
    "lint": "eslint --ext .ts ./",
    "tsc": "rm -rf ./built && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simple-Automation-Testing/promod.git"
  },
  "author": {
    "name": "Potapov Dmytro",
    "url": "https://github.com/potapovDim"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Simple-Automation-Testing/promod/issues"
  },
  "homepage": "https://github.com/Simple-Automation-Testing/promod#readme",
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.4",
    "@types/selenium-webdriver": "^4.1.9",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "assertior": "0.0.26",
    "eslint": "^7.29.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-mocha": "^8.2.0",
    "mocha": "10.0.0",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "playwright-core": "^1.28.0",
    "sat-utils": "1.3.1",
    "selenium-standalone": "^7.1.0",
    "selenium-webdriver": "4.6.1",
    "yargs": "^17.6.0"
  },
  "keywords": [
    "promod",
    "selenium-webdriver",
    "protractor",
    "protractor mod",
    "test automation",
    "selenium",
    "automation testing",
    "playwright mod"
  ]
}